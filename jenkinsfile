pipeline {
		agent any 
				stages {
						stage('One') {
								steps {
										echo "hi this is stage One"
								}
						}
						stage('Two') {
								steps {
										input('Do you want to proceed')
								}
						}
						stage('Three') {
								when {
										not {
												branch "master"
										}
								}
								steps {
										echo "Its not branch master"
								}
						}
						stage('Four') {
										stage('Unit Test') {
																	steps {
																			echo "Runing unit tests"
																	}
												}
								}
						}
						}
						
		